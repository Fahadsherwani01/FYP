@model IEnumerable<User>

@{ 
    Layout = "adminLayout";
}


<div>
    <div class="form-horizontal">
        <div class="contentBox">
            <h1 class='w3layouts-title wow fadeInDown animated well well-lg footer-bottom' style="color:white">Users</h1>

            <div class="col-md-12 welcome-grids  welcome-mdl1 wow zoomIn animated well well-lg">
                <div class="panel panel-primary">
                    <!-- Default panel contents -->
                    <div class="panel-heading"><h4 style="text-align:center">User</h4></div>
                    <div class="panel-body">
                        <p>...</p>
                    </div>  
                    <table class="table">
                        <thead>
                            <tr>
                                <th>UserName</th>
                                <th>Email</th>
                                <th>Profile</th>
                                <th>History</a></th>

                            </tr>

                        </thead>
                                                    @foreach (var item in Model)
                                                    {

                                                        <tbody>
                                                            <tr>
                                                                <td>@item.UserName</td>
                                                                <td>@item.Email</td>
                                                                <td><a class="btn btn-default admin" href="/Admin/userProfile?email=@item.Email">Profile</a></td>
                                                                <td> <a class="btn btn-default admin" href="/Admin/userCheckupHsitory?email=@item.Email">Checkup History</a></td>
                                                            </tr>
                                                         </tbody>
                           



                            }
                        </table>

                        </div>

                @*<div class="col-md-offset-2 col-md-10">

                        <input class="btn btn-info" data-wow-delay=".5s" type="submit" value="Start Over">
                    </div>*@
            </div>

        </div>
    </div>
</div>
